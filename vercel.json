{
  "version": 3,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    { "src": "/_vercel/static/(.*)", "dest": "/dist/linkSphere/browser/_vercel/static/$1" },
    { "src": "/(.*\\.(?:js|css|png|jpg|ico|json))", "dest": "/dist/linkSphere/browser/$1" },
    { "src": "/api/.*", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
